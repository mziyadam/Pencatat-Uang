<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ziyad.pencatatuang">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_dompet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_dompet_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PencatatUang"
        tools:replace="android:icon">
        <activity android:name=".Setting"></activity>
        <activity android:name=".Add" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>